alias systemctl="sudo systemctl"
alias touch="sudo touch"
alias mkdir="sudo mkdir -p"
alias chown="sudo chown -R"
alias chmod="sudo chmod -R"
alias mv="sudo mv"
alias c="clear"
alias cp="sudo cp -r"
alias setfacl="sudo setfacl -R"
alias nvim="sudo nvim"


alias -g G="grep"
alias -g GR="grep -iR"
alias -g GV="grep -iv"